  .line
  {
      height: 25px;
      line-height: 25px;
      margin: 3px;
  }
  .imp
  {
      padding-left: 25px;
  }
  .col
  {
      width: 64px;
  }
  ul {
      list-style:none;
      padding-left:10px;
  }
  li {
      height:20px;
  }
  .btn{
  	display:inline-block;
  	font-weight:400;
  	text-align:center;
  	white-space:nowrap;
  	vertical-align:middle;
  	touch-action:manipulation;
  	cursor:pointer;
  	user-select:none;
  	background-image:none;
  }
  
  .btn-group-sm>.btn, .btn-sm{
  		padding:5px 10px;
  		font-size:12px;
  		line-height:1.5;
  		border-radius:3px;
  }
  
  .btn-success{
  	color:#fff;
  	background-color:#5cb85c;
  	border-color:#4cae4c;
  }