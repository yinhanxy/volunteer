<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>tps</short-name>
  <uri>/pageTag</uri>
  
  <tag>
      <name>page</name>
       <tag-class>com.topstar.volunteer.util.tag.PageTag</tag-class>
      <body-content>empty</body-content>
	    <attribute>
